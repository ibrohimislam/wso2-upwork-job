{
  "jsxBracketSameLine": false,
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "overrides": [
    {
      "files": "*.ts",
      "options": { "parser": "typescript" }
    },
    {
      "files": "*.html",
      "options": { "parser": "html" }
    },
    {
      "files": "*.scss",
      "options": { "parser": "scss" }
    }
  ]
}
